		%link{:href => "https://fonts.googleapis.com/css?family=Dosis:400,200", :rel => "stylesheet", :type => "text/css"}/

	%body	
		.fullwrap		
			.parallax-container
				%nav
					.header
						.parallax
							%img{:src => "images/Photo/paralax/DSC_0298.jpg", :alt => ""}/
						.nav-wrapper
							%h1.brand-logo
								%a{:href => "index.html"} CHIONER
							%ul#nav-mobile.right.hide-on-med-and-down(itemscope itemtype="http://schema.org/BreadcrumbList")
								%li(itemprop="itemListElement" itemscope itemtype="http://schema.org/ListItem")
									%a.navpanel(itemprop="item" href ="index.html")
										%span(itemprop="name")
											Portfolio
									%meta(itemprop="position" content="1")		
								%li(itemprop="itemListElement" itemscope itemtype="http://schema.org/ListItem")
									%a.navpanel(itemprop="item" href ="about.html")
										%span(itemprop="name")
											About
									%meta(itemprop="position" content="2")	
								%li(itemprop="itemListElement" itemscope itemtype="http://schema.org/ListItem")
									%a.navpanel(itemprop="item" href = "contact.html")
										%span(itemprop="name")
											Contact	
									%meta(itemprop="position" content="3")			
						%ul#slide-out.side-nav(itemscope itemtype="http://schema.org/BreadcrumbList")
							%li
								%a{:href => "index.html"}Portfolio
							%li
								%a{:href => "about.html"}About
							%li
								%a{:href => "contact.html"}Contact	
							%li	
								%a(href = "https://vk.com/chioner" target="_blank")
									%img.brightness{:src => "images/title/ins.png", :alt => ""}/
								%a(href = "https://www.instagram.com/chioner/" target="_blank")
									%img.brightness{:src => "images/title/vk.png", :alt => ""}/
								%a(href = "https://www.facebook.com/profile.php?id=100011329493219"  target="_blank")
									%img.brightness{:src => "images/title/face.png", :alt => ""}/	
						%a.button-collapse.right.rightbar{"data-activates" => "slide-out", :href => "#"}
							%i.mdi-navigation-menu	
			.wrap		
				.grid
					.grid-item.widthfifty				
						%img(alt='' class='materialboxed' data-caption="Это текст подписи" src='images/Photo/People/DSC_0298.jpg')
					.grid-item.widthfifty				
						%img(alt='' class='materialboxed' data-caption="Это текст подписи" src='images/Photo/People/IMG_0194.jpg')
						
					.grid-item.widthfifty		
						%img(alt='' class='materialboxed' data-caption="Это текст подписи" src='images/Photo/People/DSC_0011.jpg')	
					
					
					
				.clearfix
			.footer
				%a(href = "https://vk.com/chioner" target="_blank")
					VK
				%a(href = "https://www.instagram.com/chioner/" target="_blank")
					INSTAGRAM
				%a(href = "https://www.facebook.com/profile.php?id=100011329493219"  target="_blank")
					FACEBOOK
				%p
					© CHIONER. All rights reserved. 2016
			.clearfix
				
		%script{:type=>"text/javascript"}
			$(document).ready(function(){
			$('.materialboxed').materialbox();});
		%script{:type=>"text/javascript"}
			$(document).ready(function(){
			$('.parallax').parallax();});
		%script{:type=>"text/javascript"}
			jQuery(document).ready(function($){
			$('.grid').masonry({ singleMode: true});

			var $container = $('.grid').masonry();
				
			$container.imagesLoaded(function(){
			$container.masonry();
			});});
		%script{:type=>"text/javascript"}
			$('.button-collapse').sideNav({
			menuWidth: 240,
			edge: 'right', 
			closeOnClick: true });	





